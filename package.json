{
  "name": "ciganpyo",
  "version": "1.2.0",
  "description": "i am lazy",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "deploy": "electron-builder --win -p always"
  },
  "build": {
    "productName": "시간표",
    "appId": "me.monun.comciganwrapper",
    "asar": true,
    "publish": [
      {
        "provider": "github",
        "owner": "issac4892",
        "repo": "ciganpyo",
        "releaseType": "release",
        "vPrefixedTagName": true
      }
    ],
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": "./resources/Icon.ico"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": false,
      "artifactName": "CiGanPyo_Setup.exe",
      "oneClick": true
    }
  },
  "license": "MIT",
  "devDependencies": {
    "builder-util": "^22.11.8",
    "electron": "^13.1.6",
    "electron-builder": "^22.11.7",
    "eslint": "^7.30.0"
  },
  "dependencies": {
    "electron-log": "^4.3.5",
    "electron-updater": "^4.3.9"
  }
}
